{
    "compilerOptions": {
        "downlevelIteration": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "target": "ES2020",
        "module": "ES2020",
        "resolveJsonModule": true,
        "jsx": "react",
        "baseUrl": "src",
        "paths": {
            "@core": [
                "core/index.ts"
            ],
            "@core/*": [
                "core/*"
            ],
            "@handlers": [
                "core/handlers/index.ts"
            ],
            "@handlers/*": [
                "core/handlers/*"
            ],
            "@modules": [
                "core/modules/index.ts"
            ],
            "@modules/*": [
                "core/modules/*"
            ],
            "@patches": [
                "core/patches/index.ts"
            ],
            "@patches/*": [
                "core/patches/*"
            ],
            "@utilities": [
                "core/utilities/index.ts"
            ],
            "@utilities/*": [
                "core/utilities/*"
            ],
            "@extension/*": [
                "../extension/*"
            ],
            "@components": [
                "core/components/index.ts"
            ],
            "@components/*": [
                "core/components/*"
            ],
            "@logger": [
                "core/logger"
            ]
        }
    }
}